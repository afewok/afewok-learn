{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.10",
    "@umijs/preset-react": "1.x",
    "lint-staged": "^10.0.7",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "umi": "^3.2.24",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@umijs/test": "^3.2.24"
  },
  "optionalDependencies": {
    "puppeteer": "^5.4.0"
  }
}
